#!/usr/bin/env julia
# echo "[\"~#'\",\"~zNaN\"][\"~#'\",\"~zNaN\"]"  && echo "[\"~#'\",\"~zNaN\"] [\"~#'\",\"~zNaN\"]" | bin/read-write && echo
# returns ["~#'","~zNaN"]["~#'","~zNaN"]

include("../src/Transit.jl")
import Transit

while !eof(STDIN)
    Transit.write(STDOUT, Transit.parse(STDIN))
end
